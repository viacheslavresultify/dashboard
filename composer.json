{
  "name": "pixelant/dashboard",
  "type": "typo3-cms-extension",
  "description": "Customizable dashboard which can display dashboard widgets.",
  "license": ["GPL-2.0+"],
  "keywords": ["TYPO3 CMS", "Dashboard"],
  "replace": {
    "dashboard": "self.version"
  },
  "autoload": {
    "psr-4": {
      "TYPO3\\CMS\\Dashboard\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TYPO3\\CMS\\Dashboard\\Tests\\": "Tests/"
    }
  }
}
